import java.util.*;
class Main{
    public static void main(String args[]){
          Scanner s = new Scanner(System.in);
          int x,y;
          System.out.println("Enter the side of the square hall");
          x=s.nextInt();
            System.out.println("Enter the side of the square table placed for display");
            y=s.nextInt();
            int area=x*x;
            int area1=y*y;
                System.out.println("Area to be decorated is "+(area-area1));}}


