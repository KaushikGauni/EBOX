    public static void main(String args[]){
          Scanner s = new Scanner(System.in);
            double sal ;
        //    int s2=(int)((sal)-800)/130;
          System.out.println("Enter the total salary paid ");
          sal = s.nextDouble();
           int s2=(int)((sal)-800)/130;
          System.out.println("Number of weekday hours is "+(s2+10));
             System.out.println("Number of weekend hours is "+s2);
    }
}

//10
import java.util.*;
import java.text.*;
class Main{
    public static void main(String args[]){
          Scanner s = new Scanner(System.in);
          int s1,s2,s3,sum;
          float r1,r2,r3;
          float rs1,rs2,rs3,avg,rsum;
          System.out.println("Enter the number of people who watched show 1");
          s1=s.nextInt();
          System.out.println("Enter the average rating for show 1");
          r1=s.nextFloat();
           System.out.println("Enter the number of people who watched show 2");
          s2=s.nextInt();
          System.out.println("Enter the average rating for show 2");
          r2=s.nextFloat();
           System.out.println("Enter the number of people who watched show 3");
          s3=s.nextInt();
          System.out.println("Enter the average rating for show 3");
          r3=s.nextFloat();
          rs1=s1*r1;
            rs2=s2*r2;
              rs3=s3*r3;
              sum=s1+s2+s3;
              rsum=rs1+rs2+rs3;
           avg=rsum/sum;
           NumberFormat df = new DecimalFormat("#0.00");
          System.out.println("The overall average rating for the show is "+df.format(avg));
             
          }
          }

//11
import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        int m,n;
        int min;
        System.out.println("Enter m");
        m=s.nextInt();
        System.out.println("Enter n");
        n=s.nextInt();
      min=m*n-1;
      System.out.println("Minimum number of times is "+min);
    }
}


//12
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int maxPackSize = 0;
        int maxLeftover = 0;

        for (int i = 1; i <= n; i++) {
            int leftover = n % i;
            if (leftover >= maxLeftover) {
                maxLeftover = leftover;
                maxPackSize = i;
            }
        }

        System.out.println(maxPackSize);
    }
}

//13
import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        int n =s.nextInt();
        int n1 = n/100;
        n=n%100;
        int n2 = n/50;
        n=n%50;
        int n3 = n/10;
        n=n%10;
        int n4 = n/5;
        n=n%5;
        int n5=n/2;
        n=n%2;
        int n6 = n/1;
        n=n%1;
        System.out.println(n1+n2+n3+n4+n5+n6);
        
    }
}


