//4

import java.util.*;
import java.text.*;
class Main{
       public static void main(String args[]){
               Scanner s = new Scanner(System.in);
               String fest;
               String t;
              double n1;    
               int n;
              char p;
               System.out.println("Enter the name of the event");
               fest=s.nextLine();
                System.out.println("Enter the type of the event");
               t=s.nextLine();
                System.out.println("Enter the number of people expected");
               n=s.nextInt();
               s.nextLine();
               System.out.println("Is it a paid entry?(Type Y or N)");
               p=s.nextLine().charAt(0);
              System.out.println("Enter the projected expenses (in lakhs) for this event");
              n1=s.nextDouble();
              DecimalFormat df = new DecimalFormat(".#");
                System.out.println("Event Name : "+fest);
                System.out.println("Event Type : "+t);
                System.out.println("Expected Count :"+n);
                 System.out.println("Paid Entry :"+p);
                System.out.println("Projected Expense : "+df.format(n1)+"L");
       }
}

//5

import java.util.*;
import java.text.*;
class Main{
       public static void main(String args[]){
               Scanner s = new Scanner(System.in);
               int a,b,c,d;
               System.out.println("Enter branding expenses ");
               a=s.nextInt();
               System.out.println("Enter travel expenses");
               b=s.nextInt();
               System.out.println("Enter food expenses");
               c=s.nextInt();
               System.out.println("Enter logistics expenses");
               d=s.nextInt();
               NumberFormat df = new DecimalFormat("#0.00");
               double sum = a+b+c+d;
              
               System.out.println("Total expenses  : Rs."+df.format(sum));
                double x =((a/sum)*100);
                double x1 =((b/sum)*100);
                double x2=((c/sum)*100);
                double x3 =((d/sum)*100);
               System.out.println("Branding expenses percentage :"+df.format(x)+"%");
               System.out.println("Travel expenses percentage :"+df.format(x1)+"%");
               System.out.println("Food expenses percentage :"+df.format(x2)+"%");
               System.out.println("Logistics expenses percentage :"+df.format(x3)+"%");
       }
}

//6
import java.util.*;
class Main{
    public static void main(String args[]){
          Scanner s = new Scanner(System.in);
          int total;
          System.out.println("Enter the total number of people ");
          total=s.nextInt();
          int x=2*(total/7);
          System.out.println("Number of attendees on day 1 :"+x);
           System.out.println("Number of attendees on day 2 :"+2*x);
            System.out.println("Number of attendees on day 3 :"+x/2);
    }
}


//7
import java.util.*;
class Main{
    public static void main(String args[]){
          Scanner s = new Scanner(System.in);
          int x,y;
        System.out.println("Enter the value of X");
        x=s.nextInt();
         System.out.println("Enter the value of Y");
        y=s.nextInt();
        int m = (y-(5*x))/13;
        System.out.println("Number of children tickets sold : "+m);
           System.out.println("Number of adult tickets sold : "+(m+x));
              System.out.println("Number of senior tickets sold : "+m*2);
    }
}


//8
import java.util.*;
class Main{
    public static void main(String args[]){
          Scanner s = new Scanner(System.in);
          int side,n;
          System.out.println("Enter the side in cm of a square tile");
          side=s.nextInt();
         System.out.println("Enter the number of square tiles available");
         n=s.nextInt();
       int maxSide = (int) Math.floor(Math.sqrt(n));
       int numTilesUsed = maxSide * maxSide;
       int area = numTilesUsed * side * side;
         System.out.println("Area of the largest possible square is "+area+"sqcm");
    }
}

