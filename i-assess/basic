import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        int x,total;
        System.out.println("Enter x");
        x=s.nextInt();
        total=x*3;
        System.out.println(total+" people were there in total");
    }
}
